-- HOME
local Home = Window:Tab({Title="HOME",Icon="house"})
local msg = ""
Home:Section({Title="ROSEY BOT v19.0 — by Rosey (21)"})
local box = Home:Input({Title="Your Message",Placeholder="Type here...",Callback=function(t)msg=t end})
Home:Button({Title="SEND",Color=Color3.fromHex("#00ff55"),Callback=function()if msg~=""then send(msg);msg="";box:SetValue("")end end})
Home:Button({Title="CLEAR",Color=Color3.fromHex("#ff5555"),Callback=function()msg="";box:SetValue("")end})

