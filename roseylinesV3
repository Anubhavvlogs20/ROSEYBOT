-- ROSEY BOT v2.0 - ULTIMATE EDITION (CREATED BY ROSEY, AGE 21)
-- Load Rayfield Interface
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local Window = Rayfield:CreateWindow({
    Name = "ROSEY BOT v2.0",
    LoadingTitle = "Rosey Loading...",
    LoadingSubtitle = "by Rosey (21)",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = "ROSEYBOTv2", -- Create a custom folder for your hub/game
        FileName = "ROSEYv2"
    },
    Discord = {
        Enabled = false, -- The Discord tab button to open Discord (false/true)
        Invite = "noinvitelink", -- The Discord invite code, do not make it your own discord link
        RememberJoins = true -- Set this to false to make them join the discord every time they load it up
    },
    KeySystem = true, -- Set this to true to use our key system
    KeySettings = {
        Title = "ROSEY BOT v2.0",
        Subtitle = "Key System",
        Note = "ðŸ‘‹ Hello! Please enter the key to access the ROSEY BOT v2.0. KEY IS: BOT",
        FileName = "ROSEYv2Key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
        SaveKey = true, -- The raw key can be saved, but if you change the key, you also have to change the SaveKey value: to true (false/true)
        GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from -> This will not work for KeySystems
        Key = "BOT" -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
    }
})

-- Beep Sound
local beep = Instance.new("Sound", game.SoundService)
beep.SoundId = "rbxassetid://6026984227"
beep.Volume = 0.7

-- Universal Chat Function
local function Chat(msg)
    pcall(function()
        if game:GetService("TextChatService").ChatVersion == Enum.ChatVersion.TextChatService then
            game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync(msg)
        else
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(msg, "All")
        end
    end)
end

-- Rainbow Text
local colors = {"#FF0000","#FF7F00","#FFFF00","#00FF00","#0000FF","#4B0082","#9400D3"}
local function rainbow(t)
    local r = ""
    for i = 1, #t do r = r .. "<font color='"..colors[(i%7)+1].."'>"..t:sub(i,i).."</font>" end
    return r
end

getgenv().RAINBOW = false

-- Send Function
local function send(text)
    local clean = text:gsub("^%[ROSEY BOT%]%s*","")
    local final = "[ROSEY BOT] "..clean
    if getgenv().RAINBOW then final = rainbow(final) end
    if setclipboard then setclipboard(final) end
    Chat(final); beep:Play()
    Rayfield:Notify({
        Title = "SENT!",
        Content = final,
        Duration = 1.8,
        Image = 4483362458,
    })
end

-- Button Creator
local function btn(t, line)
    t:CreateButton({
        Name = line,
        Callback = function() send(line) end
    })
end

-- Tab Creator with Search
local function tab(name, icon, lines)
    local t = Window:CreateTab(name, icon)
    local search = t:CreateInput({
        Name = "ðŸ” Search",
        PlaceholderText = "Filter lines...",
        Callback = function(q)
            t:ClearAllChildren()
            t:CreateInput(search)
            for _,v in lines do
                if q == "" or v:lower():find(q:lower()) then btn(t, v) end
            end
        end
    })
    for _,v in lines do btn(t, v) end
end

-- HOME TAB
local home = Window:CreateTab("ðŸ  HOME", 6023426926)
home:CreateParagraph({Title = "ROSEY BOT v2.0", Content = "Created by Rosey (21)\n800+ Lines | Type â†’ OKAY SEND | NO CLEAR"})

local userMessage = ""
local inputBox = home:CreateInput({
    Name = "ðŸ’¬ Your Message",
    PlaceholderText = "I am unstoppable! âœ¨",
    RemoveTextAfterFocusLost = false,
    Callback = function(text) userMessage = text end
})

home:CreateButton({
    Name = "âœ… OKAY SEND",
    Callback = function()
        if userMessage ~= "" then
            send(userMessage)
            userMessage = ""
            inputBox.CurrentText = ""
        else
            Rayfield:Notify({Title = "Empty! ðŸ˜¢", Content = "Write something first!", Duration = 2})
        end
    end
})

home:CreateButton({
    Name = "âŒ NO CLEAR",
    Callback = function()
        userMessage = ""
        inputBox.CurrentText = ""
        Rayfield:Notify({Title = "Cleared! âœ¨", Content = "Ready for chaos!", Duration = 1.5})
    end
})

