-- Bonus: Universal Executor (if you still want to paste custom scripts)
Home:Section({Title="UNIVERSAL EXECUTOR"})
local customScript = ""
Home:Input({Title="Custom Script",Placeholder="Paste any loadstring here...",Callback=function(t)customScript=t end})
Home:Button({
    Title = "EXECUTE CUSTOM",
    Color = Color3.fromHex("#aa00ff"),
    Callback = function()
        if customScript ~= "" then
            pcall(function() loadstring(customScript)() end)
            WindUI:Notify({Title="ROSEY BOT", Content="Custom Script Executed!", Duration=5})
        else
            WindUI:Notify({Title="ROSEY BOT", Content="No script entered!", Duration=4})
        end
    end
})

-- ULTIMATE RANK SYSTEM MODE – 150+ Ranks + Epic Messages (ROSEY BOT v19+)
local RankTab = Window:Tab({Title="Rank System", Icon="medal"})

local selectedPlayer = nil
local selectedRank = "Member"

-- Player Selector
RankTab:Section({Title="SELECT PLAYER"})
RankTab:Dropdown({
    Title = "Choose Player",
    Items = (function()
        local list = {"-- Select Player --"}
        for _, plr in ipairs(game.Players:GetPlayers()) do
            table.insert(list, plr.DisplayName.." (@"..plr.Name..")")
        end
        return list
    end)(),
    Callback = function(v)
        if v == "-- Select Player --" then selectedPlayer = nil
        else selectedPlayer = string.match(v, "@(.+)%)$") end
        WindUI:Notify({Title="ROSEY BOT", Content = selectedPlayer and "Selected: "..v or "Cleared", Duration=3})
    end
})

