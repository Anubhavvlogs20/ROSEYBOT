-- CONTROL
local Control = Window:Tab({Title="CONTROL",Icon="settings"})
Control:Toggle({Title="Rainbow Text",Default=false,Callback=function(v)getgenv().RAINBOW=v end})
Control:Button({Title="Clear Chat",Callback=function()Chat("/clear")end})

