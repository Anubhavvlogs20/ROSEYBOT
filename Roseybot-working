-- ROSEY BOT v19.0 — FULL 1000+ LINES | 25 TABS | WINDUI 2025 | 100% WORKING
local WindUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/Footagesus/WindUI/main/dist/main.lua"))()

local Window = WindUI:CreateWindow({Title="ROSEY BOT v19.0", Author="by Rosey (21)", Icon="robot"})

local function Chat(msg)
    pcall(function()
        local TCS = game:GetService("TextChatService")
        if TCS.ChatVersion == Enum.ChatVersion.TextChatService then
            TCS.TextChannels.RBXGeneral:SendAsync(msg)
        else
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(msg,"All")
        end
    end)
end

local colors = {"#FF0000","#FF7F00","#FFFF00","#00FF00","#0000FF","#4B0082","#9400D3"}
local function rainbow(t)
    local r = ""
    for i = 1,#t do r = r.."<font color='"..colors[(i%7)+1].."'>"..t:sub(i,i).."</font>" end
    return r
end
getgenv().RAINBOW = false

local function send(text)
    local final = "[ROSEY BOT] "..text:gsub("^%[ROSEY BOT%]%s*","")
    if getgenv().RAINBOW then final = rainbow(final) end
    if setclipboard then setclipboard(final) end
    Chat(final)
    WindUI:Notify({Title="SENT", Content=final, Duration=2})
end

local function Tab(name,icon,lines)
    local t = Window:Tab({Title=name,Icon=icon})
    t:Input({Title="Search",Placeholder="Filter...",Callback=function(q)
        t:Clear()
        for _,v in pairs(lines) do
            if q=="" or string.find(string.lower(v),string.lower(q)) then
                t:Button({Title=v,Callback=function()send(v)end})
            end
        end
    end})
    for _,v in pairs(lines) do t:Button({Title=v,Callback=function()send(v)end}) end
end


